{
    "name": "travisamiller/spot-tracker-api",
    "description": "An API client for Spot Tracker Shared Pages API.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Travis Miller",
            "email": "travis@travisamiller.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "~7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6",
        "php-mock/php-mock-phpunit": "^1.1",
        "guzzlehttp/guzzle": "^6.2",
        "squizlabs/php_codesniffer": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "TravisAMiller\\SpotTrackerApi\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TravisAMiller\\SpotTrackerApiTest\\": "./test"
        }
    },
    "suggest": {
        "guzzlehttp/guzzle": "A tested/trusted HTTP transport (Version ^6.2)"
    }
}
